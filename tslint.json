{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "object-literal-sort-keys": false,
    "one-line": [
      false,
      "check-catch",
      "check-finally",
      "check-else"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/**/*.d.ts"
    ]
  }
}
